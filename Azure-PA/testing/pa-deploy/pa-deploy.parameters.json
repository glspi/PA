{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vNetName": {
            "type": "string",
            "metadata": {
                "description": "Name of vNet"
            }
        },
        "fwAvSet": {
            "type": "string",
            "metadata": {
                "description": "Availability Set name for Firewalls"
            },
            "value": "PaloAlto-AS"
        },
        "privateAddressPrefix": {
            "type": "string",
            "defaultValue": "10.5.0.0/16",
            "metadata": {
                "description": "Private address prefix "
            }
        },
        "managementAddressPrefix": {
            "type": "string",
            "value": "192.168.1.0/24",
            "metadata": {
                "description": "Management address prefix "
            }
        },
        "publicAddressPrefix": {
            "type": "string",
            "defaultValue": "172.16.0.0/23",
            "metadata": {
                "description": "Public address prefix "
            }
        },
        "diagStorageAccountName": {
            "type": "string",
            "metadata": {
                "description": "Diagnostic Storage Account Name"
            }
        },
        "managementSourceAddressRange": {
            "type": "string",
            "metadata": {
                "description": "Management Security Group Source IP address range"
            }
        },
        "paloVM1Name": {
            "type": "string",
            "defaultValue": "PaloAltoVM1",
            "metadata": {
                "description": "Name of first firewall"
            }
        },
        "paloVM2Name": {
            "type": "string",
            "defaultValue": "PaloAltoVM2",
            "metadata": {
                "description": "Name of second firewall"
            }
        },
        "adminUsername": {
            "type": "string",
            "metadata": {
                "description": "Username of the administrator account of VMs"
            }
        },
        "adminPassword": {
            "type": "securestring",
            "defaultValue": "Th@nksTroy!",
            "metadata": {
                "description": "Password for the administrator account of VMs"
            }
        },
        "vmSize": {
            "type": "string",
            "allowedValues": [
                "Standard_D3",
                "Standard_D4",
                "Standard_D3_v2",
                "Standard_D4_v2",
                "Standard_A4",
                "Standard_DS3_v2",
                "Standard_DS4_v2"
            ],
            "metadata": {
                "description": "Azure VM size for VM-Series"
            },
            "defaultValue": "Standard_D3_v2"
        },
        "imageVersion": {
            "type": "string",
            "allowedValues": [
                "7.1.1",
                "8.0.0",
                "8.1.0",
                "9.0.0",
                "latest"
            ],
            "metadata": {
                "description": "PAN OS Version"
            },
            "defaultValue": "latest"
        },
        "imageSku": {
            "type": "string",
            "allowedValues": [
                "byol",
                "bundle1",
                "bundle2"
            ],
            "metadata": {
                "description": "Firewall License Model"
            }
        },
        "fw1ManagementPrivateIPAddress": {
            "type": "string",
            "defaultValue": "192.168.1.6",
            "metadata": {
                "description": "Management subnet static address for firewall"
            }
        },
        "fw2ManagementPrivateIPAddress": {
            "type": "string",
            "defaultValue": "192.168.1.7",
            "metadata": {
                "description": "Management subnet static address for firewall"
            }
        },
        "fw1PublicPrivateIPAddress": {
            "type": "string",
            "defaultValue": "172.16.1.6",
            "metadata": {
                "description": "Management subnet static address for firewall"
            }
        },
        "fw2PublicPrivateIPAddress": {
            "type": "string",
            "defaultValue": "172.16.1.7",
            "metadata": {
                "description": "Management subnet static address for firewall"
            }
        },
        "fw1PrivateIPAddress": {
            "type": "string",
            "defaultValue": "10.5.0.6",
            "metadata": {
                "description": "Management subnet static address for firewall"
            }
        },
        "fw2PrivateIPAddress": {
            "type": "string",
            "defaultValue": "10.5.0.7",
            "metadata": {
                "description": "Management subnet static address for firewall"
            }
        },
        "internalLoadBalancerIPAddress": {
            "type": "string",
            "defaultValue": "10.5.0.21",
            "metadata": {
                "description": "Internal Load Balancer IP Address"
            }
        },
        "BootstrapStorageAccount": {
            "defaultValue": "cnetpalotraining",
            "type": "string",
            "metadata": {
                "description": "Storage account hosting bootstrap files"
            }
        },
        "StorageAccountAccessKey": {
            "defaultValue": "6vXKevk8N5+pA/2CTqkKTNy9YmnLbmih3xoHd8HQQD6IsaMeAOHsOdULk/yMwI9vyjdqIQz03hVJojhCGGlWJg==",
            "type": "string",
            "metadata": {
                "description": "Authentication key to access the storage account"
            }
        },
        "StorageAccountFileShare": {
            "defaultValue": "bootstrap2",
            "type": "string",
            "metadata": {
                "description": "The file share in the storage account where the bootstrap files are located"
            }
        },
        "StorageAccountFileShareDirectory1": {
            "defaultValue": "rg/fw1",
            "type": "string",
            "metadata": {
                "description": "The subdirectory hosting the bootstrap files. Only required if hosting multiple bootstrap directories on the same share"
            }
        },
        "StorageAccountFileShareDirectory2": {
            "defaultValue": "rg/fw2",
            "type": "string",
            "metadata": {
                "description": "The subdirectory hosting the bootstrap files. Only required if hosting multiple bootstrap directories on the same share"
            }
        },
        "webLBPublicIPName": {
            "type": "string",
            "metadata": {
                "description": "Public IP for external Load Balancer"
            }
        }
    }
}